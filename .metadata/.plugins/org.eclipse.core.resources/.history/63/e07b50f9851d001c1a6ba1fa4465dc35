package fundamentos;

public class DesafioVariaveisConstantes {
	public static void main(String[] args) {
		
		// (ºF - 32) x 5/9 = ºC
		final double ajuste = 32;
		final double fator = 5.0/9.0;
		double TempF = 40.0;
		double TempFinal = (TempF - ajuste) * fator;
		
		System.out.println(TempFinal);
				
	}
}
